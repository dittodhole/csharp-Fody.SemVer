  </metadata>
  <files>
    <file src="..\bin\SemVer.Git.MSBuild\**\*.dll"
          target="tools" />
    <file src="..\bin\SemVer.Git.MSBuild\**\*.targets"
          target="" />
    <file src="..\bin\SemVer.Git.MSBuild\**\*.props"
          target="" />
  </files>
</package>