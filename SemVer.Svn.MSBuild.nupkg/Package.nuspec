<?xml version="1.0"?>
<package >
  <metadata>
    <id>SemVer.Svn.MSBuild</id>
    <version>1.1.0-alpha00</version>
    <title>SemVer.Svn.MSBuild</title>
    <authors>Andreas Niedermair</authors>
    <description>
      Versions your assemblies according to SemVer based on your SVN commit messages.
    </description>
    <projectUrl>https://github.com/dittodhole/dotnet-SemVer.Stamp</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/dittodhole/dotnet-SemVer.Stamp/master/Icons/package_icon.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/dittodhole/WTFNMFPLv3/master/LICENSE</licenseUrl>
  </metadata>
  <files>
    <file src="..\bin\SemVer.Svn.MSBuild\**\*.svnExe"
          target="tools" />
    <file src="..\bin\SemVer.Svn.MSBuild\**\*.svnDll"
          target="tools" />
    <file src="..\bin\SemVer.Svn.MSBuild\**\*.dll"
          exclude="**\SharpSvn.UI.dll"
          target="tools" />
    <file src="..\bin\SemVer.Svn.MSBuild\**\*.targets"
          target="" />
    <file src="..\bin\SemVer.Svn.MSBuild\**\*.props"
          target="" />
  </files>
</package>