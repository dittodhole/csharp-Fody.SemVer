<?xml version="1.0"?>

<package>
  <metadata>
    <id>SemVer.Git.Fody</id>
    <version>1.4.0</version>
    <title>SemVer.Git.Fody</title>
    <authors>Andreas Niedermair</authors>
    <description>
      Versions your assemblies according to SemVer based on your Git commit messages.
    </description>
    <projectUrl>https://github.com/dittodhole/csharp-Fody.SemVer</projectUrl>
    <iconUrl>https://raw.github.com/dittodhole/csharp-Fody.SemVer/master/Icons/package_icon.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/dittodhole/WTFNMFPLv3/master/LICENSE</licenseUrl>
    <tags>Git, Versioning, ILWeaving, Fody, Stamp, Cecil</tags>
    <dependencies>
      <dependency id="Fody"
                  version="1.29.4" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\bin\SemVer.Git.Fody\**\*.dll"
          target="" />
    <file src="..\bin\SemVer.Git.Fody\**\*.targets"
          target="" />
    <file src="..\bin\SemVer.Git.MSBuild\**\*.dll"
          target="tools" />
  </files>
</package>